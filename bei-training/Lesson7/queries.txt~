+-------------------+------------+-------+---------------------+------------------+
|		- Top Ten Techs by total parts cost for the month                 |
+-------------------+------------+-------+---------------------+------------------+
SELECT src.technician_number, src.tech_count, src.month, src.completion_datetime, src.parts_cost_total
FROM (
SELECT t.technician_number AS technician_number,COUNT(t.technician_id) AS tech_count, MONTH(completion_datetime) AS month, completion_datetime, SUM(sp.cost) as parts_cost_total
FROM service AS s 
JOIN technicians AS t ON s.technician_id = t.technician_id
JOIN service_parts AS sp ON sp.service_id = s.service_id
GROUP BY t.technician_number
) AS src
WHERE src.month = 3
ORDER BY src.tech_count  DESC LIMIT 10;
